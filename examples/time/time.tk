# These functions are a bit risky to use, since time_t isn't guaranteed to be an i64
extern!(i64, time, i64*);
extern!(str*, ctime, i64*);
extern_va!(i32, printf, str*);

fn main() i32 {
    i64 current_time = time(0.as!(i64*));
    str *time_str = ctime(&current_time);
    printf("Current time is %s", time_str);

    return 0;
}
